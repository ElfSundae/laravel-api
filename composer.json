{
    "name": "elfsundae/laravel-api",
    "type": "library",
    "description": "Laravel Api",
    "keywords": ["laravel","api"],
    "homepage": "https://github.com/ElfSundae/laravel-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/http": "5.*",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\Laravel\\Api\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ElfSundae\\Laravel\\Api\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "laravel": {
            "providers": [
                "ElfSundae\\Laravel\\Api\\ApiServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
